URxvt.background:       [95]#0b0b0b
URxvt.depth:		32
URxvt.font: 		xft:boxxy:size=9,\
                        xft:TerminessTTF Nerd Font Mono:size=11,\
                        xft:Symbola:size=11,\
                        xft:EmojiOne:siz=11,\
                        xft:FontAwesome:size=11,\
			xft:Noto Emoji:seize=11
URxvt.scrollBar:	false
URxvt.cursorColor:	white

!! Extensions
URxvt.perl-ext-common:	default,matcher,resize-font,url-select,keyboard-select
URxvt.colorUL:		#4682B4
!! url-select
URxvt.keysym.M-u:	perl:url-select:select_next
URxvt.url-select.launcher: webview
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! resize-font
URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up
!! Matcher
URxvt.url-launcher:	webview
URxvt.matcher.button: 1



